<p id="notice"><%= notice %></p>

<h1>Frezarimdfs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @frezarimdfs.each do |frezarimdf| %>
      <tr>
        <td><%= frezarimdf.name %></td>
        <td style="width: 300px">
          <% if frezarimdf.image_svg.attached? %>
            <%= image_tag frezarimdf.image_svg %>
          <% end %>
        </td>
        <td style="padding: 10px"><%= link_to 'Show', frezarimdf_path(frezarimdf.id), data: { turbolinks: false } %></td>
        <td style="padding: 10px"><%= link_to 'Edit', edit_frezarimdf_path(frezarimdf), data: { turbolinks: false } %></td>
        <td style="padding: 10px"><%= link_to 'Destroy', frezarimdf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Frezarimdf', new_frezarimdf_path %>
