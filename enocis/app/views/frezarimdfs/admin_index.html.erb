<%= render '/layouts/admin_panel' do %>
  <h2>Frezari MDF</h2>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Image</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @frezarimdfs.each do |frezarimdf| %>
        <tr>
          <td><%= frezarimdf.name %></td>
          <td><%= frezarimdf.image_svg %></td>
          <td><%= link_to 'Show', frezarimdf %></td>
          <td><%= link_to 'Edit', edit_frezarimdf_path(frezarimdf) %></td>
          <td><%= link_to 'Destroy', frezarimdf, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New frezarimdf', new_frezarimdf_path %>
  <%= link_to 'Back', admin_panel_path %>
<% end %>