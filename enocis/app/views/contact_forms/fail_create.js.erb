{
  document.querySelector('#BottomContact').innerHTML = "<%= escape_javascript render partial: 'contact_forms/form_footer' , locals: { contact_form: @contact_form } %>";
}
else
{
  document.querySelector('#Contact').innerHTML = "<%= escape_javascript render partial: 'contact_forms/form' , locals: { contact_form: @contact_form } %>";
}
